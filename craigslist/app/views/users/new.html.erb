<h1>Create User</h1>

<%= form_for @user do |f| %>
  <p>
  <%= f.label :email %><br>
  <%= f.email_field :email %>
  </p>
  <p>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
  </p>
  <p>
  <%= f.label :confirm_password %><br>
  <%= f.password_field :password_confirmation %>
  </p>
  <%= f.submit "Create User" %>
<% end %>
